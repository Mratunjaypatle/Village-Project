<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>All Users</title>
</head>
<body>
    <h1>All Registered Users</h1>
    <table border="black" width="500">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Age</th>
                <th>Mobile Number</th>
            </tr>
        </thead>
        <tbody>
            <% alluser.forEach(user => { %>
                <tr>
                    <td><%= user.name %></td>
                    <td><%= user.email %></td>
                    <td><%= user.age %></td>
                    <td><%= user.mobile_number %></td>
                </tr>
            <% }) %>
        </tbody>
    </table><br>
<a href="/"><button>Home</button></a>
</body>
</html>
